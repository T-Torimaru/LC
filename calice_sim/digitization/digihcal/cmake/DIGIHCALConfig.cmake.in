# DIGIHCALConfig.cmake.in
#
# cmake config file for DIGIHCAL
#  DIGIHCAL_INCLUDE_DIRS  - the DIGIHCAL include directory
#  DIGIHCAL_LIBRARIES     - The libraries needed to use DIGIHCAL
#  DIGIHCAL_DEFINITIONS   - the compile time definitions


#fixme: not true if INCLUDE_INSTALL_DIR is absolute path
SET( DIGIHCAL_INCLUDE_DIRS "@FULL_INCLUDE_INSTALL_DIR@/" )
SET( DIGIHCAL_LIBRARIES    "@FULL_LIB_INSTALL_DIR@/@FULL_LIBRARY_NAME@" )

SET( DIGIHCAL_DEFINITIONS @DIGIHCAL_EXTERN_DEFINITIONS@ )

SET( DIGIHCAL_ROOT_DIR "@CMAKE_INSTALL_PREFIX@" )

SET( DIGIHCAL_MAJOR_VERSION "@DIGIHCAL_MAJOR_VERSION@" )
SET( DIGIHCAL_MINOR_VERSION "@DIGIHCAL_MINOR_VERSION@" )
SET( DIGIHCAL_PATH_LEVEL    "@DIGIHCAL_PATCH_LEVEL@"    )


SET( DIGIHCAL_FOUND TRUE )

