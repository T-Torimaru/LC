# GANGINGConfig.cmake.in
#
# cmake config file for GANGING
#  GANGING_INCLUDE_DIRS  - the GANGING include directory
#  GANGING_LIBRARIES     - The libraries needed to use GANGING
#  GANGING_DEFINITIONS   - the compile time definitions


#fixme: not true if INCLUDE_INSTALL_DIR is absolute path
SET( GANGING_INCLUDE_DIRS "@FULL_INCLUDE_INSTALL_DIR@/" )
SET( GANGING_LIBRARIES    "@FULL_LIB_INSTALL_DIR@/@FULL_LIBRARY_NAME@" )

SET( GANGING_DEFINITIONS @GANGING_EXTERN_DEFINITIONS@ )

SET( GANGING_ROOT_DIR "@CMAKE_INSTALL_PREFIX@" )

SET( GANGING_MAJOR_VERSION "@GANGING_MAJOR_VERSION@" )
SET( GANGING_MINOR_VERSION "@GANGING_MINOR_VERSION@" )
SET( GANGING_PATH_LEVEL    "@GANGING_PATCH_LEVEL@"    )


SET( GANGING_FOUND TRUE )

