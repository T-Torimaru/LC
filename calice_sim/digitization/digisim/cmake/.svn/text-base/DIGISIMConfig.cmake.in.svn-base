# DIGISIMConfig.cmake.in
#
# cmake config file for DIGISIM
#  DIGISIM_INCLUDE_DIRS  - the DIGISIM include directory
#  DIGISIM_LIBRARIES     - The libraries needed to use DIGISIM
#  DIGISIM_DEFINITIONS   - the compile time definitions


#fixme: not true if INCLUDE_INSTALL_DIR is absolute path
SET( DIGISIM_INCLUDE_DIRS "@FULL_INCLUDE_INSTALL_DIR@/" )
SET( DIGISIM_LIBRARIES    "@FULL_LIB_INSTALL_DIR@/@FULL_LIBRARY_NAME@" )

SET( DIGISIM_DEFINITIONS @DIGISIM_EXTERN_DEFINITIONS@ )

SET( DIGISIM_ROOT_DIR "@CMAKE_INSTALL_PREFIX@" )

SET( DIGISIM_MAJOR_VERSION "@DIGISIM_MAJOR_VERSION@" )
SET( DIGISIM_MINOR_VERSION "@DIGISIM_MINOR_VERSION@" )
SET( DIGISIM_PATH_LEVEL    "@DIGISIM_PATCH_LEVEL@"    )


SET( DIGISIM_FOUND TRUE )

