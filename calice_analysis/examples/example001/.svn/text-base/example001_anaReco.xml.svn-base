<marlin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ilcsoft.desy.de/marlin/marlin.xsd">


  <!--
      Example steering file used for the analysis presented in CAN-034 and PhD thesis "Low-energetic Hadron Interactions in a Highly Granular
      Calorimeter" by N. Feege (in preparation).
      The analysis is based on data collected with the AHCAL and the TCMT (without any ECAL) at Fermilab in July 2008 and May 2009.
      The example run 520308 is a 10 GeV negative pion run.
  -->


  <!-- Order of processor execution -->

  <execute>

    <processor name="ProgressHandler"/>

    <processor name="Conditions"/>

    <processor name="MultiBitGenerator"/>

    <processor name="EventSelector"/>

    <processor name="AhcMappingProcessor"/>
    <processor name="AhcCellDescriptionProcessor"/>
    <processor name="AhcCellNeighboursProcessor"/>

    <processor name="AhcRemoveBadCellsProcessor"/>
    <processor name="AhcNoiseCut"/>

    <processor name="AhcVetoRegion"/>

    <processor name="AhcShowerStartClusterProcessor"/>
    <processor name="AhcConvertPositionToLayer"/>
    <processor name="AhcShowerStartClusterPattern"/>

    <processor name="AhcCollectClusterTypeB"/>
    <processor name="AhcClusterTypeBPattern"/>

    <processor name="AhcMipTrackFinder"/>

    <processor name="AhcPrimaryTrackFinder"/>

    <processor name="AhcConvertAmplitudeProcessor"/>
    <processor name="AhcRemoveBadCellsProcessor_noTCorr"/>
    <processor name="AhcNoiseCut_noTCorr"/>

    <processor name="TcmtNoiseCut"/>
    <processor name="TcmtMuonTracker"/>

    <processor name="AhcAppendIntegralObservables"/>
    <processor name="AhcAppendLongitudinalObservables"/>

    <processor name="AhcVirtualCellsProcessor"/>
    <processor name="AhcProfileProcessor"/>

    <processor name="AhcRootTreeWriter"/>
    <processor name="AhcRootTreeWriter_noTCorr"/>

    <processor name="AhcRootTreeWriter_lowMipCut"/>
    <processor name="AhcRootTreeWriter_highMipCut"/>

    <processor name="TcmtRootTreeWriter"/>

    <processor name="LCIOOutput"/>

  </execute>



  <!--- Set processor parameters -->

  <global>
    <parameter name="LCIOInputFiles">
      Run520308_rec.000.slcio
    </parameter>

    <parameter name="MaxRecordNumber" value="2000" />
    <parameter name="SkipNEvents" value="0" />
    <parameter name="SupressCheck" value="false" />
    <parameter name="Verbosity" options="DEBUG0-4,MESSAGE0-4,WARNING0-4,ERROR0-4,SILENT"> WARNING </parameter>
  </global>


  <processor name="ProgressHandler" type="ProgressHandler">
    <parameter name="ReportInterval" type="int" value="20"/>
  </processor>


  <processor name="Conditions" type="ConditionsProcessor">
    <parameter name="DBInit" type="string" value="flccaldb01.desy.de:calice:caliceon:Delice.1:3306"/>
    <parameter name="DBCondHandler" type="StringVec">
      AhcModuleLocationReference /cd_calice_fnalbeam/Hcal/HcalModuleLocationReference  ahc_HcalModuleLocationReference_001
      AhcModuleConnection        /cd_calice_fnalbeam/Hcal/HcalMapping                  ahc_HcalMapping_002
      AhcModuleDescription       /cd_calice/Ahc/ModuleDescription                      ahc_ModuleDescription_001
      AhcDetectorTransformation  /cd_calice_fnalbeam/Hcal/DetectorPosition             ahc_DetectorPosition_002
      AhcCellQuality             /cd_calice/Ahc/BadCellMap                             ahc_bad_cell_map_003
    </parameter>
  </processor>


  <processor name="MultiBitGenerator" type="MultiBitGenerator">
    <parameter name="parNameMultiAmplitude" type="string">MultiAmplitude </parameter>
    <parameter name="parNameMultiBit" type="string">MultiBit </parameter>
    <parameter name="purity" type="float">99.9 </parameter>
    <parameter name="threshold" type="float">3800 </parameter>
    <parameter name="thresholdCollectionName" type="string">MultiThreshold </parameter>
    <parameter name="useFixedThreshold" type="bool">true </parameter>
  </processor>


  <processor name="EventSelector" type="eventSelector">
    <parameter name="MultiBitName" type="string">MultiBit </parameter>
    <parameter name="TriggerConfigurationName" type="string">Conf_Trigger </parameter>
    <parameter name="TriggerEventName" type="string">Event_Trigger </parameter>
    <parameter name="withoutTrigger" type="StringVec"> Ca </parameter>
  </processor>


  <processor name="AhcMappingProcessor" type="MappingProcessor">
    <parameter name="ModuleConnection"  type="string" lcioInType="LCGenericObject"> AhcModuleConnection </parameter>
    <parameter name="ModuleDescription" type="string" lcioInType="LCGenericObject"> AhcModuleDescription </parameter>
    <parameter name="MapperType" type="string" value="AHC"/>
  </processor>


  <processor name="AhcCellDescriptionProcessor" type="CellDescriptionProcessor">
    <parameter name="MappingProcessorName"   type="string"> AhcMappingProcessor </parameter>
    <parameter name="DetectorTransformation" type="string" lcioInType="LCGenericObject"> AhcDetectorTransformation </parameter>
    <parameter name="ModuleConnection"       type="string" lcioInType="LCGenericObject"> AhcModuleConnection </parameter>
    <parameter name="ModuleDescription"      type="string" lcioInType="LCGenericObject"> AhcModuleDescription </parameter>
    <parameter name="ModuleLocation"         type="string" lcioInType="LCGenericObject"> AhcModuleLocationReference </parameter>
  </processor>


  <processor name="AhcCellNeighboursProcessor" type="CellNeighboursProcessor">
    <parameter name="MappingProcessorName" type="string">AhcMappingProcessor </parameter>
  </processor>


  <processor name="AhcRemoveBadCellsProcessor" type="RemoveBadCells">
    <parameter name="AHC_HitColName_in" type="string">AhcCalorimeter_Hits </parameter>
    <parameter name="AHC_HitColName_rmBad" type="string">AhcCalorimeter_Hits_rmBadCells </parameter>
    <parameter name="CellQualityColName" type="string">AhcCellQuality </parameter>
    <parameter name="MappingProcessorName" type="string">AhcMappingProcessor </parameter>
  </processor>


  <processor name="AhcNoiseCut" type="ApplyThresholdCut">
    <parameter name="AHC_HitColName_afterCut" type="string">AhcCalorimeter_Hits_rmBadCells_noiseCut </parameter>
    <parameter name="AHC_HitColName_beforeCut" type="string">AhcCalorimeter_Hits_rmBadCells </parameter>
    <parameter name="MipCut" type="float">0.5 </parameter>
  </processor>

  <processor name="AhcVetoRegion" type="AhcVetoRegion">
    <parameter name="AHC_HitColName" type="string">AhcCalorimeter_Hits </parameter>
    <parameter name="CellDescriptionProcessorName" type="string">AhcCellDescriptionProcessor </parameter>
    <parameter name="MappingProcessorName" type="string">AhcMappingProcessor </parameter>
    <parameter name="roiCells" type="IntVec">             18    18    18    18    18    3     12         </parameter>
    <parameter name="roiFirstLayer" type="IntVec">        1     1     1     1     1     1     1          </parameter>
    <parameter name="roiLastLayer" type="IntVec">         1     2     3     4     5     5     38         </parameter>
    <parameter name="roiInnerSideLength" type="FloatVec"> 0     0     0     0     0     0     0          </parameter>
    <parameter name="roiOuterSideLength" type="FloatVec"> 5000  5000  5000  5000  5000  5000  5000       </parameter>
    <parameter name="roiInnerRadius" type="FloatVec">     0     0     0     0     0     0     0          </parameter>
    <parameter name="roiOuterRadius" type="FloatVec">     5000  5000  5000  5000  5000  5000  5000       </parameter>
    <parameter name="HitThreshold" type="FloatVec">       0.5   0.5   0.5   0.5   0.5   0.5   0.5        </parameter>
    <parameter name="minESum" type="FloatVec">            0     0     0     0     0     0     0          </parameter>
    <parameter name="minHits" type="IntVec">              1     1     1     1     1     1     1          </parameter>
  </processor>


  <processor name="AhcShowerStartClusterProcessor" type="ShowerStartClusterProcessor">
    <parameter name="AhcCollection" type="string" lcioInType="CalorimeterHit">AhcCalorimeter_Hits_rmBadCells_noiseCut </parameter>
    <parameter name="CellNeighboursProcessorName" type="string">AhcCellNeighboursProcessor </parameter>
    <parameter name="EnergyThreshold" type="FloatVec">16 10 </parameter>
    <parameter name="HitThreshold" type="IntVec">4 6 </parameter>
    <parameter name="MappingProcessorName" type="string">AhcMappingProcessor </parameter>
    <parameter name="MaximumAngleDeviation" type="FloatVec">90 90 </parameter>
    <parameter name="MinHitsCluster" type="int">2 </parameter>
    <parameter name="PositionOfIP" type="FloatVec">0 0 0  </parameter>
    <parameter name="SeedThreshold" type="float">1.65 </parameter>
    <parameter name="StopAfterFirstMatch" type="int">1 </parameter>
  </processor>


  <processor name="AhcConvertPositionToLayer" type="ConvertPositionToLayer">
    <parameter name="CellDescriptionProcessorName" type="string">AhcCellDescriptionProcessor </parameter>
    <parameter name="InputPisitionParamterName" type="string">AhcShowerStartClusterProcessor_showerStartPos </parameter>
    <parameter name="MappingProcessorName" type="string">AhcMappingProcessor </parameter>
  </processor>


  <processor name="AhcShowerStartClusterPattern" type="ClusterPatternProcessor">
    <parameter name="ExplicitColNames" type="StringVec"> </parameter>
    <parameter name="MinEnergyCluster" type="float">1 </parameter>
    <parameter name="MinHitsCluster" type="int">2 </parameter>
    <parameter name="StringColNames" type="string"> AhcShowerStartClusterProcessor_candidates </parameter>
  </processor>


  <processor name="AhcCollectClusterTypeB" type="ShowerStartClusterProcessor">
    <parameter name="AhcCollection" type="string" lcioInType="CalorimeterHit">AhcCalorimeter_Hits_rmBadCells_noiseCut </parameter>
    <parameter name="CellNeighboursProcessorName" type="string">AhcCellNeighboursProcessor </parameter>
    <parameter name="EnergyThreshold" type="FloatVec">16 </parameter>
    <parameter name="HitThreshold" type="IntVec">4 </parameter>
    <parameter name="MappingProcessorName" type="string">AhcMappingProcessor </parameter>
    <parameter name="MaximumAngleDeviation" type="FloatVec">90 </parameter>
    <parameter name="MinHitsCluster" type="int">2 </parameter>
    <parameter name="PositionOfIP" type="FloatVec">0 0 0  </parameter>
    <parameter name="SeedThreshold" type="float">1.0 </parameter>
    <parameter name="StopAfterFirstMatch" type="int">0 </parameter>
  </processor>


  <processor name="AhcClusterTypeBPattern" type="ClusterPatternProcessor">
    <parameter name="ExplicitColNames" type="StringVec"> </parameter>
    <parameter name="MinEnergyCluster" type="float">1 </parameter>
    <parameter name="MinHitsCluster" type="int">2 </parameter>
    <parameter name="StringColNames" type="string"> AhcCollectClusterTypeB_candidates </parameter>
  </processor>


  <processor name="AhcMipTrackFinder" type="MipTrackFinder">
    <parameter name="AHCALBadModules" type="IntVec"> </parameter>
    <parameter name="AHCALFirstModules" type="IntVec"> </parameter>
    <parameter name="AHCALInputCollectionName" type="string">AhcCalorimeter_Hits_rmBadCells_noiseCut </parameter>
    <parameter name="AHCALLastModules" type="IntVec"> </parameter>
    <parameter name="AHCALTrackOutputCollectionName" type="string">AhcTrackClusters </parameter>
    <parameter name="AmplitudeHCALInputCollectionName" type="string"> </parameter>
    <parameter name="CellDescriptionProcessorName" type="string">AhcCellDescriptionProcessor </parameter>
    <parameter name="ECALInputCollectionName" type="string"> </parameter>
    <parameter name="IsNonMuonRun" type="bool">true </parameter>
    <parameter name="MappingProcessorName" type="string">AhcMappingProcessor </parameter>
    <parameter name="MaxAHCALNoHits" type="int">10000 </parameter>
    <parameter name="MaxECALNoHits" type="int">10000 </parameter>
    <parameter name="MinECALNoHits" type="int">0 </parameter>
    <parameter name="MinTCMTEnergySum" type="float">0 </parameter>
    <parameter name="MipCutValue" type="float">0.5 </parameter>
    <parameter name="MipCutVariable" type="int">1 </parameter>
    <parameter name="TCMInputCollectionName" type="string"> </parameter>
    <parameter name="TrackNoHitsThreshold" type="int">20 </parameter> <!-- default 20 -->
    <parameter name="TrackSelectionMode" type="string">NHITS </parameter>
    <parameter name="UseECALandTCMT" type="bool">false </parameter>
  </processor>


  <processor name="AhcPrimaryTrackFinder" type="PrimaryTrackFinder">
    <parameter name="AbsTrackLengthLimit" type="int">3 </parameter>
    <parameter name="BeamAngle" type="float">0 </parameter>
    <parameter name="BeamEnergy" type="float">0 </parameter>
    <parameter name="CEDLineSize" type="int">2 </parameter>
    <parameter name="CEDPointSize" type="int">5 </parameter>
    <parameter name="CED_HostPort" type="int">7557 </parameter>
    <parameter name="DrawFlag" type="bool">false </parameter>
    <parameter name="FixStartingLayer" type="bool">false </parameter>
    <parameter name="NoiseMIPLimit" type="float">0.5 </parameter>
    <parameter name="OutputStartCollectionName" type="string">ShowerStartingLayer </parameter>
    <parameter name="OutputTrackCollectionName" type="string">PrimaryTrackHits </parameter>
    <parameter name="RelTrackLengthLimit" type="float">0.3 </parameter>
    <parameter name="RootOutputFileDir" type="string">./ </parameter>
    <parameter name="RootOutputFlag" type="bool">false </parameter>
    <parameter name="SizeofGAP" type="int">6 </parameter>
    <parameter name="SkipTrackFinder" type="bool">false </parameter>
    <parameter name="XAlignment" type="float">0 </parameter>
    <parameter name="YAlignment" type="float">0 </parameter>
    <parameter name="ecalHitsCollection" type="string"> </parameter>
    <parameter name="hcalHitsCollection" type="string"> AhcCalorimeter_Hits_rmBadCells_noiseCut </parameter>
    <parameter name="tcmtHitsCollection" type="string"> TcmtCalorimeter_Hits_noiseCut </parameter>
  </processor>


  <processor name="AhcConvertAmplitudeProcessor" type="ConvertAmpl2CaloHitProcessor">
    <parameter name="AhcAmplCollectionName" type="string">AhcAmplitude </parameter>
    <parameter name="AhcHitCollectionName" type="string">AhcCalorimeter_Hits </parameter>
    <parameter name="HitAmplRelationName" type="string">AhcHitAmplitudeRelation </parameter>
    <parameter name="OutputAhcHitV2CollectionName" type="string">AhcCalorimeter_Hits_noTCorr </parameter>
    <parameter name="V2AmplitudeName" type="string">NOTTemperatureCorrMIP </parameter>
  </processor>


  <processor name="AhcRemoveBadCellsProcessor_noTCorr" type="RemoveBadCells">
    <parameter name="AHC_HitColName_in" type="string">AhcCalorimeter_Hits_noTCorr </parameter>
    <parameter name="AHC_HitColName_rmBad" type="string">AhcCalorimeter_Hits_noTCorr_rmBadCells </parameter>
    <parameter name="CellQualityColName" type="string">AhcCellQuality </parameter>
    <parameter name="MappingProcessorName" type="string">AhcMappingProcessor </parameter>
  </processor>


  <processor name="AhcNoiseCut_noTCorr" type="ApplyThresholdCut">
    <parameter name="AHC_HitColName_afterCut" type="string">AhcCalorimeter_Hits_noTCorr_rmBadCells_noiseCut </parameter>
    <parameter name="AHC_HitColName_beforeCut" type="string">AhcCalorimeter_Hits_noTCorr_rmBadCells </parameter>
    <parameter name="MipCut" type="float">0.5 </parameter>
  </processor>


  <processor name="TcmtNoiseCut" type="ApplyThresholdCut">
    <parameter name="AHC_HitColName_afterCut" type="string">TcmtCalorimeter_Hits_noiseCut </parameter>
    <parameter name="AHC_HitColName_beforeCut" type="string">TcmtCalorimeter_Hits </parameter>
    <parameter name="MipCut" type="float">0.5 </parameter>
  </processor>


  <processor name="TcmtMuonTracker" type="TcmtMuonTracker">
    <parameter name="MaxLayerK" type="int">16 </parameter>
    <parameter name="MipThreshold" type="float">0.5 </parameter>
    <parameter name="TCMT_HitColName" type="string"> TcmtCalorimeter_Hits_noiseCut </parameter>
    <parameter name="Verbosity" type="string"> WARNING </parameter>
    <parameter name="mu_eSum_isolated" type="FloatVec">0 0 0 0  0 </parameter>
    <parameter name="mu_nHits_isolated" type="IntVec"> 2 4 6 8 10 </parameter>
  </processor>


  <processor name="AhcAppendIntegralObservables" type="AppendIntegralObservables">
    <parameter name="HitCollection" type="string" lcioInType="CalorimeterHit">AhcCalorimeter_Hits_rmBadCells_noiseCut </parameter>
  </processor>


  <processor name="AhcAppendLongitudinalObservables" type="AppendLongitudinalObservables">
    <parameter name="CellDescriptionProcessorName" type="string"> AhcCellDescriptionProcessor </parameter>
    <parameter name="HitCollection" type="string" lcioInType="CalorimeterHit"> AhcCalorimeter_Hits_rmBadCells_noiseCut </parameter>
    <parameter name="MappingProcessorName" type="string"> AhcMappingProcessor </parameter>
    <parameter name="CenterOfGravityXYZParameterName" type="string"> AhcAppendIntegralObservables_cogXYZ5Layer </parameter>
  </processor>


  <processor name="AhcVirtualCellsProcessor" type="VirtualCellsProcessor">
    <parameter name="CellDescriptionProcessorName" type="string" value="AhcCellDescriptionProcessor"/>
    <parameter name="MappingProcessorName"         type="string" value="AhcMappingProcessor"/>
  </processor>


  <group>

    <parameter name="MappingProcessorName"         type="string" value="AhcMappingProcessor"/>
    <parameter name="CellDescriptionProcessorName" type="string" value="AhcCellDescriptionProcessor"/>
    <parameter name="VirtualCellsProcessorName"    type="string" value="AhcVirtualCellsProcessor"/>
    <parameter name="ShowerStartProcessorName"     type="string" value="AhcShowerStartClusterProcessor"/>
    <parameter name="CenterOfGravityParameterName"     type="string" value="AhcAppendIntegralObservables_cogXYZ5Layer"/>
    <parameter name="AhcSimCollection"             type="string" lcioInType="SimCalorimeterHit"> </parameter>
    <parameter name="MiddleCellIndex" type="IntVec"> 45 45 1 45 46 1 46 45 1 46 46 1 </parameter>
    <parameter name="MiddleCellIndexBack" type="IntVec"> 45 45 38 </parameter>
    <parameter name="RBinning" type="FloatVec">  30 600 </parameter>
    <parameter name="ZRange"   type="IntVec"  > -50  50 </parameter>
    <parameter name="ZOffset"  type="FloatVec"> 0 </parameter>>
    <parameter name="SuppressOutsideFirstAndLastLayerCenter" type="int" value="1"/>

    <processor name="AhcProfileProcessor" type="CalorimeterProfileProcessor">
      <parameter name="AhcCollection"    type="string" lcioInType="CalorimeterHit">    AhcCalorimeter_Hits_rmBadCells_noiseCut </parameter>
      <parameter name="Threshold" type="float" value="0.5"/>
    </processor>

  </group>


