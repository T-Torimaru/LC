# CALICEANALYSISConfig.cmake.in
#
# cmake config file for CALICEANALYSIS
#  CALICEANALYSIS_INCLUDE_DIRS  - the CALICEANALYSIS include directory
#  CALICEANALYSIS_LIBRARIES     - The libraries needed to use CALICEANALYSIS
#  CALICEANALYSIS_DEFINITIONS   - the compile time definitions


#fixme: not true if INCLUDE_INSTALL_DIR is absolute path
SET( CALICEANALYSIS_INCLUDE_DIRS "@FULL_INCLUDE_INSTALL_DIRS@" )
SET( CALICEANALYSIS_LIBRARIES    "@FULL_LIB_INSTALL_DIR@/@FULL_LIBRARY_NAME@" )

SET( CALICEANALYSIS_DEFINITIONS @CALICEANALYSIS_EXPORT_DEFINITIONS@ )

SET( CALICEANALYSIS_ROOT_DIR "@CMAKE_INSTALL_PREFIX@" )

SET( CALICEANALYSIS_MAJOR_VERSION "@CALICEANALYSIS_MAJOR_VERSION@" )
SET( CALICEANALYSIS_MINOR_VERSION "@CALICEANALYSIS_MINOR_VERSION@" )
SET( CALICEANALYSIS_PATH_LEVEL    "@CALICEANALYSIS_PATCH_LEVEL@"    )


SET( CALICEANALYSIS_FOUND TRUE )

