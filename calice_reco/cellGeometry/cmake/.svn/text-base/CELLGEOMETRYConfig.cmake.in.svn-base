# CELLGEOMETRYConfig.cmake.in
#
# cmake config file for CELLGEOMETRY
#  CELLGEOMETRY_INCLUDE_DIRS  - the CELLGEOMETRY include directory
#  CELLGEOMETRY_LIBRARIES     - The libraries needed to use CELLGEOMETRY
#  CELLGEOMETRY_DEFINITIONS   - the compile time definitions


#fixme: not true if INCLUDE_INSTALL_DIR is absolute path
SET( @PROJECT_NAME@_INCLUDE_DIRS "@FULL_INCLUDE_INSTALL_DIR@" )
SET( @PROJECT_NAME@_LIBRARIES    "@FULL_LIB_INSTALL_DIR@/@FULL_LIBRARY_NAME@" )

SET( @PROJECT_NAME@_DEFINITIONS @CELLGEOMETRY_EXPORT_DEFINITIONS@ )

SET( @PROJECT_NAME@_ROOT_DIR "@CMAKE_INSTALL_PREFIX@" )

SET( @PROJECT_NAME@_MAJOR_VERSION "@CELLGEOMETRY_MAJOR_VERSION@" )
SET( @PROJECT_NAME@_MINOR_VERSION "@CELLGEOMETRY_MINOR_VERSION@" )
SET( @PROJECT_NAME@_PATH_LEVEL    "@CELLGEOMETRY_PATCH_LEVEL@"    )


SET( @PROJECT_NAME@_FOUND TRUE )

