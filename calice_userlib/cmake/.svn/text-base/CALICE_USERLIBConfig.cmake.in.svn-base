# CALICE_USERLIBConfig.cmake.in
#
# cmake config file for CALICE_USERLIB
#  CALICE_USERLIB_INCLUDE_DIRS  - the CALICE_USERLIB include directory
#  CALICE_USERLIB_LIBRARIES     - The libraries needed to use CALICE_USERLIB
#  CALICE_USERLIB_DEFINITIONS   - the compile time definitions


#fixme: not true if INCLUDE_INSTALL_DIR is absolute path
SET( CALICE_USERLIB_INCLUDE_DIRS "@FULL_INCLUDE_INSTALL_DIR@" )
SET( CALICE_USERLIB_INCLUDE_DIRS "${CALICE_USERLIB_INCLUDE_DIRS}"
                                  "@FULL_INCLUDE_INSTALL_DIR@/Mapping"
                                  "@FULL_INCLUDE_INSTALL_DIR@/CellDescription" 
                                  "@FULL_INCLUDE_INSTALL_DIR@/Neighbours")

SET( CALICE_USERLIB_LIBRARIES "@FULL_LIB_INSTALL_DIR@/@FULL_LIBRARY_NAME@" )

SET( CALICE_USERLIB_DEFINITIONS @CALICE_USERLIB_EXPORT_DEFINITIONS@ )

SET( CALICE_USERLIB_ROOT_DIR "@CMAKE_INSTALL_PREFIX@" )

SET( CALICE_USERLIB_MAJOR_VERSION "@CALICE_USERLIB_MAJOR_VERSION@" )
SET( CALICE_USERLIB_MINOR_VERSION "@CALICE_USERLIB_MINOR_VERSION@" )
SET( CALICE_USERLIB_PATH_LEVEL    "@CALICE_USERLIB_PATCH_LEVEL@"    )


SET( CALICE_USERLIB_FOUND TRUE )

