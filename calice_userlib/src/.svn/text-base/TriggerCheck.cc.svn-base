#include "TriggerCheck.hh"

namespace CALICE {

void TriggerCheck::print(  std::ostream& os ){

  os << " main_word: " <<   getMainWord() << std::endl;
  os << " Tolerance: " <<   getTolerance() << std::endl;
  os << " upwards_search: " <<   getUpSearch() << std::endl;
  os << " downwards_search: " <<   getDownSearch() << std::endl;

}

}
