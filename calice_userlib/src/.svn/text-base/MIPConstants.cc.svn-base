#include "MIPConstants.hh"

using namespace lcio;

namespace CALICE {

void MIPConstants::print(std::ostream& os) {

  os << " chip: " << std::hex << getChip() << ", channel: " << getChannel() << " " << getCellKey() << std::dec
     << ", MIP calibration: " << getMIPValue() << "+-" << getMIPError() << std::endl;

};

}
