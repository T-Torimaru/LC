#include <DriftChamberParameter.hh>

namespace CALICE {
  void DriftChamberParameter::print(std::ostream &os)
  {
    os << "z-pos=" << getOffsetZ()
       << " axis offset=" << getOffsetU()
       << " drift direction=" << getDirectionSign()
       << " pos. error=" << getError()
       << " drift velcity=" << getDriftVelocity()
       << " delay=" << getDelay();
  }
}
