#include "InterConstants.hh"

using namespace lcio;

namespace CALICE {

void InterConstants::print(std::ostream& os) {

  os << " chip: " << std::hex << getChip() << ", channel: " << getChannel() << " " << getCellKey() << std::dec
     << ", Inter calibration: " << getInterValue() << "+-" << getInterError() << std::endl;

};

}
