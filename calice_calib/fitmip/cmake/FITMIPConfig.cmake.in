# FITMIPConfig.cmake.in
#
# cmake config file for FITMIP
#  FITMIP_INCLUDE_DIRS  - the FITMIP include directory
#  FITMIP_LIBRARIES     - The libraries needed to use FITMIP
#  FITMIP_DEFINITIONS   - the compile time definitions

SET( FITMIP_INCLUDE_DIRS "@FULL_INCLUDE_INSTALL_DIR@" )
SET( FITMIP_LIBRARIES    "@FULL_LIB_INSTALL_DIR@/@FULL_LIBRARY_NAME@" )

SET( FITMIP_DEFINITIONS @FITMIP_EXPORT_DEFINITIONS@ )

SET( FITMIP_ROOT_DIR "@CMAKE_INSTALL_PREFIX@" )

SET( FITMIP_MAJOR_VERSION "@FITMIP_MAJOR_VERSION@" )
SET( FITMIP_MINOR_VERSION "@FITMIP_MINOR_VERSION@" )
SET( FITMIP_PATH_LEVEL    "@FITMIP_PATCH_LEVEL@"    )


SET( FITMIP_FOUND TRUE )

