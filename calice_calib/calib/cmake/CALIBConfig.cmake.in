# CALIBConfig.cmake.in
#
# cmake config file for CALIB
#  CALIB_INCLUDE_DIRS  - the CALIB include directory
#  CALIB_LIBRARIES     - The libraries needed to use CALIB
#  CALIB_DEFINITIONS   - the compile time definitions

SET( CALIB_INCLUDE_DIRS "@FULL_INCLUDE_INSTALL_DIR@" )
SET( CALIB_LIBRARIES    "@FULL_LIB_INSTALL_DIR@/@FULL_LIBRARY_NAME@" )

SET( CALIB_DEFINITIONS @CALIB_EXPORT_DEFINITIONS@ )

SET( CALIB_ROOT_DIR "@CMAKE_INSTALL_PREFIX@" )

SET( CALIB_MAJOR_VERSION "@CALIB_MAJOR_VERSION@" )
SET( CALIB_MINOR_VERSION "@CALIB_MINOR_VERSION@" )
SET( CALIB_PATH_LEVEL    "@CALIB_PATCH_LEVEL@"    )


SET( CALIB_FOUND TRUE )

