# LABVIEW2LCIOConfig.cmake.in
#
# cmake config file for LABVIEW2LCIO
#  LABVIEW2LCIO_INCLUDE_DIRS  - the LABVIEW2LCIO include directory
#  LABVIEW2LCIO_LIBRARIES     - The libraries needed to use LABVIEW2LCIO
#  LABVIEW2LCIO_DEFINITIONS   - the compile time definitions


#fixme: not true if INCLUDE_INSTALL_DIR is absolute path
SET( LABVIEW2LCIO_INCLUDE_DIRS "@FULL_INCLUDE_INSTALL_DIR@" )

SET( LABVIEW2LCIO_LIBRARIES "@FULL_LIB_INSTALL_DIR@/@FULL_LIBRARY_NAME@" )

SET( LABVIEW2LCIO_DEFINITIONS @LABVIEW2LCIO_EXPORT_DEFINITIONS@ )

SET( LABVIEW2LCIO_ROOT_DIR "@CMAKE_INSTALL_PREFIX@" )

SET( LABVIEW2LCIO_MAJOR_VERSION "@LABVIEW2LCIO_MAJOR_VERSION@" )
SET( LABVIEW2LCIO_MINOR_VERSION "@LABVIEW2LCIO_MINOR_VERSION@" )
SET( LABVIEW2LCIO_PATH_LEVEL    "@LABVIEW2LCIO_PATCH_LEVEL@"    )


SET( LABVIEW2LCIO_FOUND TRUE )

